<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bruno.prototipo11">

    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".CalibracaoActivity"/>
        <activity android:name=".PreCalibracao"/>
        <activity android:name=".OuvidoDireito500Activity" />
        <activity android:name=".OuvidoDireito1kActivity"/>
        <activity android:name=".OuvidoDireito2kActivity"/>
        <activity android:name=".OuvidoDireito4kActivity"/>
        <activity android:name=".OuvidoDireito8kActivity"/>
        <activity android:name=".OuvidoEsquerdo500Activity"/>
        <activity android:name=".OuvidoEsquerdo1kActivity"/>
        <activity android:name=".OuvidoEsquerdo2kActivity"/>
        <activity android:name=".OuvidoEsquerdo4kActivity"/>
        <activity android:name=".OuvidoEsquerdo8kActivity"/>
        <activity android:name=".Fim1Activity"/>
        <activity android:name=".Fim2Activity" />
        <activity android:name=".Fim3Activity"/>
        <activity android:name=".Fim3EsqActivity"/>
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>

</manifest>